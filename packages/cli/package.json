{
  "name": "@siberiacancode/reactuse-cli",
  "type": "module",
  "description": "Add hooks to your apps.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/siberiacancode/reactuse.git",
    "directory": "packages/cli"
  },
  "keywords": [
    "hooks",
    "reactuse",
    "reactuse-cli",
    "react",
    "react hooks",
    "react use",
    "use",
    "hooks"
  ],
  "exports": "./dist/index.js",
  "bin": {
    "reactuse-cli": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "generate-registry": "tsx --tsconfig ./tsconfig.scripts.json scripts/generateRegistry.ts",
    "update-registry": "tsx --tsconfig ./tsconfig.scripts.json scripts/updateRegistry.ts",
    "start:dev": "cross-env COMPONENTS_REGISTRY_URL=http://localhost:5173/reactuse/registry.json node dist/index.js",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "acorn": "^8.14.0",
    "acorn-typescript": "^1.4.13",
    "chalk": "5.2.0",
    "cosmiconfig": "^9.0.0",
    "cross-env": "^7.0.3",
    "fs-extra": "^11.2.0",
    "ora": "^6.1.2",
    "please-upgrade-node": "^3.2.0",
    "prompts": "^2.4.2",
    "screenfull": "^6.0.2",
    "tsconfig-paths": "^4.2.0",
    "yargs": "^17.7.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.0.2",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.32",
    "ts-node": "^10.9.2",
    "tsup": "^6.6.3",
    "tsx": "^4.19.2",
    "typescript": "^4.9.3"
  }
}
